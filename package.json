{
  "name": "typescript-template",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "dev": "node --enable-source-maps -r @swc-node/register -r tsconfig-paths/register ./src/index.ts",
    "nodemon": "nodemon",
    "test": "jest --passWithNoTests",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:format": "npm run fix:prettier",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix:lint": "eslint \"src/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yundohyun/typescript-template.git"
  },
  "author": "yundohyun",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.10",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  }
}
